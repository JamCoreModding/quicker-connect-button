{
    "schemaVersion": 1,
    "id": "quickerconnectbutton",
    "version": "${version}",
    "name": "Quicker Connect Button",
    "description": "Adds a connect button to the main menu for quickly connecting to servers",
    "authors": [
        "Jamalam"
    ],
    "contact": {
        "homepage": "https://github.com/JamCoreModding/quicker-connect-button",
        "sources": "https://github.com/JamCoreModding/quicker-connect-button",
        "issues": "https://github.com/JamCoreModding/quicker-connect-button/issues"
    },
    "license": "MIT",
    "icon": "assets/quickerconnectbutton/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "io.github.jamalam360.quickerconnectbutton.fabric.QuickerConnectButtonFabric"
        ]
    },
    "mixins": [
        "quickerconnectbutton.mixins.json"
    ],
    "depends": {
        "fabric": ">=${fabric_api_version}",
        "minecraft": ">=${minecraft_version}",
        "architectury": ">=${architectury_version}",
        "jamlib": ">=${jamlib_version}"
    }
}
