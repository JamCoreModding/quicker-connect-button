{
  "schema_version": 1,
  "mixin": [
    "quickerconnectbutton.mixins.json"
  ],
  "quilt_loader": {
    "group": "io.github.jamalam360",
    "id": "quickerconnectbutton",
    "version": "${version}",
    "metadata": {
      "name": "Quicker Connect Button",
      "description": "Adds a connect button to the main menu for quickly connecting to servers",
      "contributors": {
        "Jamalam": "Owner"
      },
      "contact": {
        "sources": "https://github.com/JamCoreModding/quicker-connect-button"
      },
      "icon": "assets/quickerconnectbutton/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "client_init": ["io.github.jamalam360.quickerconnectbutton.quilt.QuickerConnectButtonQuilt"]
    },
    "depends": [
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": ">=${architectury_version}"
      },
      {
        "id": "jamlib",
        "version": ">=${jamlib_version}"
      }
    ]
  },
  "minecraft": {
    "environment": "client"
  }
}
